{
  "name": "sync-javascript",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate:env": "env-cmd --silent -f ./.env node ./scripts/env.js",
    "build": "rm -rf dist && mkdir dist && yarn generate:env && cp -r ./node_modules/@portabl/js-sync-with-portabl/dist/index.umd.js ./dist/index.umd.js",
    "dev": "yarn build && http-server -p 3001",
    "lint": "eslint ."
  },
  "dependencies": {
    "axios": "^0.27.2",
    "@portabl/js-sync-with-portabl": "^0.1.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint-config-custom": "*",
    "http-server": "^14.1.1"
  },
  "workspaces": {
    "nohoist": [
      "@portabl/js-sync-with-portabl"
    ]
  }
}
