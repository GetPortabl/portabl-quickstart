{
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**", "public/dist/**"],
      "dependsOn": ["^build"],
      "env": [
        "JS_APP_PUBLIC_API_HOST",
        "JS_APP_PUBLIC_PORTABL_PUBLIC_CLIENT_ID",
        "JS_APP_PUBLIC_SYNC_ACCEPT_URL",
        "JS_APP_PUBLIC_PASSPORT_URL",
        "JS_APP_AUTH0_API_AUDIENCE",
        "JS_APP_AUTH0_DOMAIN"
      ]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
