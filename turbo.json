{
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**", "public/dist/**"],
      "dependsOn": ["^build"],
      "env": [
        "JS_APP_PUBLIC_API_HOST",
        "JS_APP_WIDGET_BASE_URL",
        "JS_APP_PORTABL_ACCOUNT_ID",
        "JS_APP_PORTABL_CONNECT_DOMAIN",
        "JS_APP_PORTABL_WALLET_DOMAIN"
      ]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
